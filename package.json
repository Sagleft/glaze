{
  "name": "glaze",
  "version": "0.3.5",
  "description": "A simple and fast P2P voice chat for gaming",
  "main": "main.js",
  "scripts": {
    "pack": "build --dir",
    "distwindows": "build -w",
    "distlinux": "build -l",
    "distmac": "build -m",
    "dist": "build -mwl",
    "start": "electron .",
    "temp": "build -w --ia32",
    "pub": "build -mwl -p always",
    "release": "build"
  },
  "repository": "https://github.com/adamsiwiec/glaze",
  "author": "Adam Siwiec",
  "license": "MIT",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.5.0",
    "electron-builder": "^19.8.0",
    "webpack": "^3.0.0"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "adamsiwiec",
        "repo": "glaze"
      }
    ],
    "appId": "com.adamsiwiec.glaze",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "tar.gz"
    }
  },
  "dependencies": {
    "css-loader": "^0.28.4",
    "electron-updater": "^2.7.1",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.3.1",
    "jquery": "^3.1.1",
    "style-loader": "^0.18.2"
  }
}
